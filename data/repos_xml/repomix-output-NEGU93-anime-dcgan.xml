        function loadimg() {

            $('.cover').animate({opacity: 1}, 100, function() {

                //finished animating, minifade out and fade new back in           
                $('.cover').animate({opacity: 0.2}, 300, function() {

                    currimg++;
                    $(this).find('.cf-item').removeClass('active');
