    } if(settings.foursquare_user!='' && settings.foursquare_token!=''){ 
      foursquare(); 
    } if(settings.tumblr_username!=''){ 
      tumblr(); 
    } if(settings.twitch_username!='' && settings.twitch_client_id!=''){ 
      twitch(); 
