    // Moz Clicked
    var isDragging = false;
    $(".moz-item").mousedown(function(e) {
        // isDragging = false;
        // console.log(e);
        $(window).mousemove(function(e) {
            isDragging = true;
            // console.log(isDragging);
            $(window).unbind("mousemove");
        });
    }).mouseup(function(e) {
        // console.log(e.which);
        if (e.which === 1) {
