        # detector options
        faces = faceCascade.detectMultiScale(gray,
                                             scaleFactor=1.01,
                                             minNeighbors=5,
                                             minSize=(90, 90))
        # if any faces are detected, we only extract the biggest detected region
        if len(faces) == 0:
            continue
        elif len(faces) > 1:
            sorted(faces, key=biggest_rectangle, reverse=True)

